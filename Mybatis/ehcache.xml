<?xml version="1.0" encoding="UTF-8" ?>
<ehcache>
    <diskStore path="D:\\CACHE"/>
    <defaultCache
        maxElementsInMemory="10000"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="true"
        />
</ehcache>
<!-- EHCache缓存配置属性解释
	path
		* 缓存中对象超出范围,序列化到硬盘的路径.如果文件夹不存在,框架会自动创建
	maxElementsInMemory
		* 在内存中最大可以存储多少个对象
	eternal
		* 是否永久缓存对象
	timeToIdleSeconds
		* 缓存中对象,多久没有使用就会被清理掉
	timeToLiveSeconds
		* 缓存中对象,活跃多久就会被清理掉.
	overflowToDisk
		* 超过缓存,是否存储到磁盘
 -->
